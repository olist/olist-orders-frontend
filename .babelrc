{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": [
          "pt",
          "en"
        ],
        "outputPath": "public/locales/{{locale}}/{{ns}}.json",
        "keyAsDefaultValue": true,
        "discardOldKeys": true,
        "keySeparator": null,
        "nsSeparator": null
      }
    ]
  ]
}
